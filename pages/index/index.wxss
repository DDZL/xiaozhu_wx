/**index.wxss**/

button{
  background: rgb(247, 65, 65) !important;
  color:white !important;
  border-radius: 30px;
  width:83%;
  height:80rpx;
  line-height: 38px;
}
.date-two{
  color:red;
  border-bottom: 1px solid rgb(192, 191, 191);
  margin:20px;
  padding: 10px 0 15px 0;
  display: flex;
}
.cnaps>text{
  color:black;
  font-size: 12px;
  line-height: 30px;
  text-align: right;
}
.cnaps{
  color:rgb(247, 65, 65);
  border-bottom: 1px solid rgb(192, 191, 191);
  margin:20px;
  padding-bottom: 15px;
  display: flex;
}
.section{
  background: #fff;
  width: 70%;
  padding-bottom: 20rpx;
  position: absolute;
  top:50%;
  left: 15%;
  border-radius: 10px;
  box-shadow:0px 10px 8px 3px rgb(44, 44, 44);
}
.xz_index>image{
  width: 95%;
  height: 550px;
  border-radius: 30px;
}
.xz_index{
  text-align: center;
}
page {
  height: 100%;
}
 
.view {
  width: 100%;
  height: 100%;
}
 
map {
  width: 100%;
  height: 100%;
  background-color: rgb(247, 65, 65);
}
.w-35b{
  width:40%;
}
.w-50b{
  width:50%;
}
/* 文本样式 */
.font-24r{
  font-size: 24rpx;
}
.text-black{
  color:#000;
}
.text-center{
  text-align: center;
}
.text-left{
  text-align: left;
}
.text-right{
  text-align: right;
}

/* 媒体查询 */
@media screen and (max-width: 320px) {
      .section {
          top: 50%;
        }
        .xz_index>image{
          height:445px;
          margin-top: 22%;
        }
    }
    @media screen and (width: 360px) {
      .section {
          top: 55%;
        }
        .xz_index>image{
          height:509px;
          margin-top: 19%;
        }
    }
    @media screen and (width: 375px) {
      .section {
          top: 60%;
        }
        .xz_index>image{
          height:543px;
          margin-top: 19%;
        }
    }


